{
  "name": "starmap",
  "version": "1.0.0",
  "description": "An interactive map of the Spore galaxy",
  "main": "app.js",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "pkg -c package.json app.js"
  },
  "keywords": [
    "spore",
    "galaxy",
    "editor",
    "interactive",
    "data",
    "visualizaion"
  ],
  "author": "TheOnlyZac",
  "license": "GPL-3.0",
  "pkg": {
    "scripts": [
      "backend/src/**/*"
    ],
    "assets": [
      "public/**/*",
      "backent/bin/**/*"
    ],
    "targets": [
      "node12-win-x64",
      "node12-macos-x64",
      "node12-linux-x64"
    ],
    "outputPath": "build"
  }
}
